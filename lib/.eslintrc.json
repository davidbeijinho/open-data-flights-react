{
  "extends": ["airbnb", "prettier", "plugin:node/recommended", "plugin:import/errors", "plugin:import/warnings"],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": { 
    "prettier/prettier": ["error"],
    "arrow-parens":["error", "always"],
    "import/extensions": ["error", "always"]

  },
  "plugins":["prettier", "import"]
}
