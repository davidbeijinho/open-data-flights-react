{
  "name": "open-data-flights-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://davidbeijinho.github.io/open-data-flights-react",
  "dependencies": {
    "JSONStream": "^1.3.5",
    "cpy-cli": "^2.0.0",
    "csv-parse": "^4.3.3",
    "d3": "^5.9.1",
    "d3-scale": "^2.2.2",
    "datamaps": "^0.5.9",
    "del-cli": "^1.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "event-stream": "^4.0.1",
    "gh-pages": "^2.0.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "multi-progress": "^2.0.0",
    "prettier": "^1.16.4",
    "progress-stream": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "react-table": "^6.9.2",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "winston": "^3.2.1"
  },
  "scripts": {
    "deleteBuildData": "del build/data",
    "copyRoutesData": "cpy 'data/processed' 'build/data/processed/'",
    "copyAirportsData": "cpy 'data/airports/filtred-airports.json' 'build/data/airports/'",
    "copyCountriesData": "cpy 'data/countries/filtred-countries.json' 'build/data/countries/'",
    "copyData": "npm run deleteBuildData && npm run copyRoutesData && npm run copyAirportsData && npm run copyCountriesData",
    "predeploy": "npm run build && npm run copyData",
    "deploy": "gh-pages -d build",
    "fulldeploy": "npm run buildData && npm run deploy",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lintClient": "eslint src/",
    "lintNode": "eslint lib/ ",
    "lintClient:fix": "eslint src/ --fix",
    "lintNode:fix": "eslint lib/ --fix",
    "lint": "npm run lintNode && npm run lintClient ",
    "lint:fix": "npm run lintNode:fix && npm run lintClient:fix",
    "prettier": "npm run prettierClient && npm run prettierNode",
    "prettier:check": "npm run prettierClient:check && npm run prettierNode:check",
    "prettierClient": "prettier lib/**.js --write",
    "prettierClient:check": "prettier --check lib/**.js",
    "prettierNode": "prettier lib/**.js --write",
    "prettierNode:check": "prettier --check lib/**.js",
    "downloadRoutes": "node ./lib/downloadRoutes.js",
    "downloadAirports": "node ./lib/downloadAirports.js",
    "downloadCountries": "node ./lib/downloadCountries.js",
    "decompressRoutes": "node ./lib/decompressRoutes.js",
    "parseRoutes": "node ./lib/parseRoutes.js",
    "generateAirportsData": "node ./lib/generateAirportsData.js",
    "generateCountriesData": "node ./lib/generateCountriesData.js",
    "buildData": "npm run downloadRoutes && npm run downloadAirports && npm run downloadCountries &&  npm run decompressRoutes &&  npm run parseRoutes && npm run generateAirportsData && npm run generateCountriesData"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=8.15.0"
  }
}
